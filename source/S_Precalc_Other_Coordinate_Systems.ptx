      plot11+=line([(0,0),(1.5,-1.5)],color="blue",thickness=2)+text("$P=(r,\\theta) \\ =(\\sqrt{3},-\\arctan(\\frac{\\sqrt{2}}{2}))$",(1.5,-1.75),fontsize=15,color="blue")
