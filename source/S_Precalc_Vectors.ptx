            plot4+=arrow((0,0),(1,-1),color="black",width=3)
            plot4+=text("$\\vec{u}$",(1,2),color="black",fontsize=20)
            plot4+=text("$\\vec{v}$",(0.3,-0.75),color="black",fontsize=20)
            plot4+=arrow((0,0),(3,2),color="black",width=3)
