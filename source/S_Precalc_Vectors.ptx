            plot6=arrow((0,0),(2,3),color="black",width=3,axes=True,ticks=[[],[]],xmin=-0.5,xmax=3.5,ymin=-1.5,ymax=3.5,aspect_ratio=1)
            plot6+=arrow((0,0),(1,-1),color="black",width=3)
            plot6+=text("$\\vec{u}$",(1,2),color="black",fontsize=20)
            plot6+=text("$\\vec{v}$",(0.3,-0.75),color="black",fontsize=20)
            plot6+=arrow((0,0),(3,2),color="black",width=3)
            plot6+=text("$\\vec{u}+\\vec{v}$",(1.5,.5),color="black",fontsize=20)
            plot6+=polygon([(0,0),(2,3),(3,2),(1,-1),(0,0)],color="gray",alpha=0.25,edgecolor="black")
            plot6+=arrow((2,3),(3,2),color="gray",width=1.5,zorder=0)
            plot6+=text("$\\vec{v}$",(2.75,2.75),color="gray",fontsize=20)
            plot6+=arrow((1,-1),(3,2),color="gray",width=1.5,zorder=0)
